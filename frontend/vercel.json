{
    "builds": [
      {
        "src": "src/App.jsx",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/events/(.*)",
        "dest": "/events/[id]"
      }
    ],
    "env": {
      "VITE_API_BASE_URL": "https://event-reservation-1-lpob.onrender.com/api"
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Permissions-Policy",
            "value": "camera=(), microphone=()"
          }
        ]
      }
    ]
  }